class Main
{
	function void main()
	{
		var int number;
     		var int fact, i;
		let number = Keyboard.readInt("Enter a number ");
      		let i = 1;
		let fact=1;
		while(i < (number+1)) 
		{
			let fact=fact*i;
			let i=i+1;
		}
		do Output.printString("The factorial is ");
      		do Output.printInt(fact);
		return;
	}
}